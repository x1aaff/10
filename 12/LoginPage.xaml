<Page x:Class="_12.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:_12"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoginPage">

    <Grid Background="Black">
        <ComboBox x:Name="cbox_login" HorizontalAlignment="Center" Margin="0,171,0,0" VerticalAlignment="Top" Width="228" Height="29" />
        <Button x:Name="but_login" Click="but_login_Click" Content="Войти" HorizontalAlignment="Center" Margin="0,250,0,0" VerticalAlignment="Top" Height="29" Width="228"/>
        <CheckBox x:Name="chckbox_generate" Content="сгенерировать данные" HorizontalAlignment="Center" Margin="0,295,0,0" VerticalAlignment="Top" Height="21" Width="228" Background="White" Foreground="White"/>
    </Grid>
</Page>
